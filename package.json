{
  "name": "fullstack-engineer-challenge-datastore",
  "version": "1.0.0",
  "description": "This is the datastore / api for the Feather Fullstack Engineer Code Challenge",
  "main": "index.js",
  "scripts": {
    "createDB": "psql postgres -d feather_fullstack_code_challenge -f ./db/createDatabase.sql",
    "insertData": "psql postgres -d feather_fullstack_code_challenge -f ./db/insertData.sql",
    "herokuDbReset": "heroku pg:reset HEROKU_POSTGRESQL_ONYX --confirm e-commerce-rest-api-v2",
    "herokuDbPush": "heroku pg:push codecademy_ecommerce_rest_api_v2 HEROKU_POSTGRESQL_ONYX --app e-commerce-rest-api-v2",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
