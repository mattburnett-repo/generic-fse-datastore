{
  "name": "fullstack-engineer-challenge-datastore",
  "version": "1.0.0",
  "description": "This is the datastore / api for the Feather Fullstack Engineer Code Challenge",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "dev": "nodemon ./index.js",
    "createDB": "psql postgres -d feather_fullstack_code_challenge -f ./db/createDatabase.sql",
    "insertData": "psql postgres -d feather_fullstack_code_challenge -f ./db/insertData.sql",
    "herokuDbReset": "heroku pg:reset --app feather-datastore",
    "herokuDbPush": "heroku pg:push feather_fullstack_code_challenge DATABASE_URL --app feather-datastore",
    "test": "mocha -w",
    "testOnce": "mocha -b --exit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "js-yaml": "^4.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "serve-favicon": "^2.5.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "dotenv": "^16.0.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
  }
}
